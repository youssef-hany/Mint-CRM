{
	"name": "mint-crm",
	"version": "0.0.1",
	"author": "Youssef Hany Aref <youssefhany30@gmail.com>",
	"description": "An application built for Customer Relation Management for Mint Cleaning Services",
	"repository": "git@github.com:youssef-hany/Mint-CRM.git",
	"license": "LICENSE",
	"build": {
		"appId": "mint.system",
		"win": {
			"target": [
				"nsis"
			],
			"publish": [
				{
					"provider": "github",
					"owner": "youssef-hany",
					"repo": "Mint-CRM",
					"private": true
				}
			],
			"icon": "src/icon.ico",
			"requestedExecutionLevel": "requireAdministrator"
		},
		"nsis": {
			"installerIcon": "src/icon.ico",
			"uninstallerIcon": "src/icon.ico",
			"uninstallDisplayName": "Uninstall",
			"license": "LICENSE",
			"warningsAsErrors": false,
			"oneClick": false,
			"allowToChangeInstallationDirectory": true,
			"include": "installer.nsh"
		}
	},
	"homepage": "./",
	"main": "public/electron.js",
	"dependencies": {
		"@progress/kendo-react-dropdowns": "^3.18.0",
		"@progress/kendo-react-intl": "^3.18.0",
		"@syncfusion/ej2-react-schedule": "^19.1.69",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"axios": "^0.19.2",
		"core-js": "^3.6.5",
		"cross-env": "^7.0.2",
		"date-and-time": "^0.14.1",
		"electron-browser-storage": "^1.0.7",
		"electron-is-dev": "^1.2.0",
		"electron-log": "^4.3.1",
		"electron-updater": "^4.3.5",
		"file-saver": "^2.0.2",
		"jsonwebtoken": "^8.5.1",
		"raf": "^3.4.1",
		"react": "^16.14.0",
		"react-app-polyfill": "^1.0.6",
		"react-bootstrap": "^1.3.0",
		"react-datepicker": "^3.3.0",
		"react-dom": "^16.14.0",
		"react-loader-spinner": "^3.1.14",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^4.0.0",
		"react-time-picker": "^4.3.0",
		"react-to-print": "^2.10.1",
		"react-virtualized-auto-sizer": "^1.0.2",
		"react-window": "^1.8.5",
		"regenerator-runtime": "^0.13.7",
		"rimraf": "^3.0.2",
		"semantic-ui-react": "^1.3.1",
		"web-vitals": "^2.0.1",
		"xlsx": "^0.16.8"
	},
	"scripts": {
		"react-start": "react-scripts start",
		"react-build": "react-scripts build",
		"react-test": "react-scripts test",
		"react-eject": "react-scripts eject",
		"electron-build": "electron-builder",
		"electron-build-publish": "electron-builder --x64 --win -p always",
		"build": "npm run react-build && npm run electron-build",
		"gh-publish": "npm run react-build && npm run electron-build-publish",
		"start": "concurrently \"cross-env BROWSER=none npm run react-start \" \"wait-on http://localhost:3000 && electron .\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version",
			"ie 11"
		]
	},
	"devDependencies": {
		"concurrently": "^5.3.0",
		"electron": "^9.3.2",
		"electron-builder": "^22.9.1",
		"wait-on": "^5.2.0"
	}
}
